{
  "name": "kitzberger/tight",
  "type": "typo3-cms-extension",
  "description": "Tight Backend: Tweaks the backend CSS of TYPO3 **13.4** to reduce the vast space set between the UI elements.",
  "homepage": "https://github.com/kitzberger/tight",
  "license": [
    "GPL-2.0-or-later"
  ],
  "keywords": [
    "TYPO3 CMS"
  ],
  "require": {
    "typo3/cms-core": "^13.4"
  },
  "autoload": {
    "psr-4": {
      "Kitzberger\\Tight\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "tight"
    }
  }
}
